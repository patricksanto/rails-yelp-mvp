<div class="container">
  <div class="card shadow p-4 m-4">
    <h1>New review</h1>
    <%= simple_form_for @restaurant do |f| %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.input :category %>
      <%= f.submit "CREATE", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
